{
  "name": "xliffmanager",
  "productName": "XLIFF Manager",
  "version": "1.8.0",
  "description": "XLIFF Manager",
  "main": "js/app.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && electron .",
    "package-mac": "electron-packager --overwrite --darwinDarkModeSupport=true --icon icons/openxliff.icns  --app-bundle-id=com.maxprograms.xliffmanager --ignore 'docs' --ignore 'ts' .",
    "package-win": "electron-packager --overwrite --icon icons\\openxliff.ico --ignore 'docs' --ignore 'ts' .",
    "package-linux": "electron-packager --overwrite --icon icons/openxliff.png --ignore 'docs' --ignore 'ts' ."
  },
  "keywords": [
    "xliff",
    "filters",
    "convert",
    "merge",
    "statistics"
  ],
  "author": "Rodolfo M. Raya",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/rmraya/XLIFFManager.git"
  },
  "devDependencies": {
    "electron": "^8.2.0",
    "typescript": "^3.8.3"
  }
}
